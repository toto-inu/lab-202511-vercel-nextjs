{
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(tree:*)",
      "WebSearch",
      "Bash(bunx prisma generate:*)",
      "Bash(docker compose:*)",
      "Bash(bun run dev)",
      "mcp__playwright__browser_navigate",
      "mcp__playwright__browser_take_screenshot"
